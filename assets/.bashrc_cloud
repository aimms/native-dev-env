alias k=kubectl

if [[ "$SHELL" == "/bin/zsh" ]]; then
  # shellcheck disable=SC1090
  source <(kubectl completion zsh)
else
  source <(kubectl completion bash)
fi

cloud_info(){
  info_for_app az
  info_for_app terraform
}

info(){
  main_info
  cloud_info
  # clear color
  echo -e "\033[0m"
}

echo "${normal}Type ${bold}'info' ${normal}for the image information."
# clear color
echo -e "\033[0m"
