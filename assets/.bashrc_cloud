alias k=kubectl

# shellcheck disable=SC1090
source <(kubectl completion zsh)

cloud_info(){
  echo " ${color_red}az:\n  ${color_blue} type 'a az && az --version' for version information"
  info_for_app terraform

  # clear color_blue
  if [ ! -e $DEVENV_THEMING ]; then
    echo -e "\033[0m"
  fi
}

info(){
  main_info
  cloud_info
  alias_info
}

# clear color_blue
if [ ! -e $DEVENV_THEMING ]; then
  echo -e "\033[0m"
fi
